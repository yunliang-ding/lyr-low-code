    {
      "widget": "Switch",
      "name": "disabled",
      "label": "是否禁用"
    },
    {
      "widget": "Switch",
      "name": "allowClear",
      "label": "是否允许清除"
    },
    {
      "widget": "Input",
      "name": "placeholder",
      "label": "提示文案"
    },
    {
      "widget": "CodeEditor",
      "label": "设置options",
      "name": "initOptions",
      "props": {
        "defaultCode": "async (form) => { \n  return [] \n}",
        "mode": "function",
        "useEncrypt": true
      }
    },
    {
      "widget": "CodeEditor",
      "name": "onChange",
      "label": "设置onChange",
      "props": {
        "mode": "function",
        "useEncrypt": true
      }
    }
  ]
