{
  "widget": "AsyncSelect",
  "label": "异步选择框",
  "props": {
    "placeholder": "请选择",
    "allowClear": true,
    "disabled": false
  },
  "propsConfig": [
    {
      "widget": "Switch",
      "name": "disabled",
      "label": "是否禁用"
    },
    {
      "widget": "Switch",
      "name": "allowClear",
      "label": "是否允许清除"
    },
    {
      "widget": "Input",
      "name": "placeholder",
      "label": "提示文案"
    },
    {
      "widget": "Select",
      "name": "mode",
      "label": "模式",
      "props": {
        "allowClear": true,
        "options": [
          {
            "label": "multiple",
            "value": "multiple"
          },
          {
            "label": "tags",
            "value": "tags"
          }
        ]
      }
    },
    {
      "widget": "CodeEditor",
      "label": "设置options",
      "name": "options",
      "props": {
        "defaultCode": "async (form) => { \n  return [] \n}",
        "mode": "function",
        "useEncrypt": true
      }
    },
    {
      "widget": "CodeEditor",
      "name": "onChange",
      "label": "设置onChange",
      "props": {
        "mode": "function",
        "useEncrypt": true
      }
    }
  ]
}
